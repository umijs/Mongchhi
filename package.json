    "eslint": "^8.35.0",
    "father": "^4.1.5",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "lerna": "^6.5.1",
    "mcs": "workspace:*",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "start-server-and-test": "^1.15.3",
    "stylelint": "^15.2.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "packageManager": "pnpm@7.24.2"
